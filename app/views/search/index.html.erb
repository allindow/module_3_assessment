<h1><%= @stores.first.total %> Total Stores</h1>
<ul class="stores-list">
  <% @stores.each do |store| %>
    <li>
      Name: <%= link_to store.long_name, store_path(store.id) %><br>
      City: <%= store.city %><br>
      Distance: <%= store.distance %><br>
      Phone Number: <%= store.phone %><br>
      Store Type: <%= store.store_type %>
    </li>
  <% end %>
</ul>
